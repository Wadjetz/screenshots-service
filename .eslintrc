{
  "rules": {
    "indent": [1, 2],
    "quotes": [1, "double"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-console": [0],
    "react/jsx-uses-react": 1,
  },
  "env": {
    "es6": true,
    "node": false,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}

